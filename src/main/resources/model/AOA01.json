    "configs": [
        {
            "identifier": "mqtt",
            "title": "MQTT Connection",
            "description": "MQTT Connection",
            "category": "config",
            "dataType": {
                "type": "object",
                "definition": {
                    "object definition": [
                        {
                            "identifier": "host",
                            "title": "Host",
                            "description": "Secondary MQTT Connection Host",
                            "dataType": {
                                "type": "string"
                            }
                        },
                        {
                            "identifier": "port",
                            "title": "Port",
                            "description": "Secondary MQTT Connection Port",
                            "dataType": {
                                "type": "int",
                                "definition": {
                                    "min": null,
                                    "max": null,
                                    "unit": null,
                                    "accuracy": null
                                }
                            }
                        },
                        {
                            "identifier": "usr",
                            "title": "Username",
                            "description": "Secondary MQTT Connection Username",
                            "dataType": {
                                "type": "string"
                            }
                        },
                        {
                            "identifier": "pwd",
                            "title": "Password",
                            "description": "Secondary MQTT Connection Password",
                            "dataType": {
                                "type": "string"
                            }
                        },
                        {
                            "identifier": "tls",
                            "title": "TLS Switch",
                            "description": "Secondary MQTT Connection TLS Switch",
                            "dataType": {
                                "type": "boolean",
                                "definition": {
                                    "boolean descriptions": {
                                        "0": "Off",
                                        "1": "On"
                                    }
                                }
                            }
                        }
                    ]
                },
                "validation": {
                    "default": "{\"host\":\"${mqtt_host}\",\"port\":1883,\"usr\":\"${domain}\",\"pwd\":\"tmxpwd\",\"tls\":0}",
                    "method": "{\"host\":null,\"port\":\"area\",\"usr\":null,\"pwd\":null,\"tls\":\"switch\"}",
                    "check": "{\"host\":null,\"port\":\"{\\\"min\\\":1,\\\"max\\\":65535}\",\"usr\":null,\"pwd\":null,\"tls\":null}",
                    "category detail": "{\"name\": \"sec_mqtt_connection_settings\", \"title\": \"Secondary MQTT Connection Settings\"}"
                }
            }
        },
        {
            "identifier": "heartbeat_timer",
            "title": "Heartbeat timer",
            "description": "<p>MQTT Keepalive timer, <p>specified in seconds (s), default 300s/5min.",
            "category": "config",
            "specification": {
                "type": "int",
                "definition": {
                    "min": null,
                    "max": null,
                    "unit": "s",
                    "accuracy": null
