            },
            "validation": {
                "default": "300",
                "method": "area",
                "check": "{\"min\":120,\"max\":3600}",
                "category detail": "{\"name\": \"heartbeat_settings\", \"title\": \"Heartbeat\"}"
            }
        },
        {
            "identifier": "report_timer",
            "title": "Report Interval",
            "description": "<p>Reporting interval when the terminal is externally powered,</p><p>specified in seconds (s), default 900s/15min.</p>",
            "category": "config",
            "specification": {
                "type": "int",
                "definition": {
                    "min": null,
                    "max": null,
                    "unit": "s",
                    "accuracy": null
                }
            },
            "validation": {
                "default": "900",
                "method": "area",
                "check": "{\"min\":30,\"max\":1814400}",
                "category detail": "{\"name\": \"ac_power_mode_settings\", \"title\": \"AC Power Mode\"}"
            }
        },
        {
            "identifier": "locate",
            "title": "Positioning Mode Definition",
            "description": "<p>Positioning Mode Definition, default null,</p><p>which means to switch off positioning feature.</p>",
            "category": "config",
            "specification": {
                "type": "enum",
                "definition": {
                    "enum descriptions": {
                        "5": "GNSS"
                    }
                }
            },
            "validation": {
                "default": null,
                "method": "checkbox",
                "check": null,
                "category detail": "{\"name\": \"positioning_settings\", \"title\": \"Positioning\"}"
            }
        },
        {
            "identifier": "tmin",
            "title": "Minimum Retry Timer",
            "description": "<p>Minimum retry timer,</p><p>specified in seconds (s), default 5s.</p>",
            "category": "config",
            "specification": {
                "type": "int",
                "definition": {
                    "min": null,
                    "max": null,
                    "unit": "s",
                    "accuracy": null
                }
            },
            "validation": {
                "default": "5",
                "method": "area",
                "check": "{\"min\":1,\"max\":1814400}",
                "category detail": "{\"name\": \"retry_settings\", \"title\": \"Retry\"}"
            }
        },
        {
            "identifier": "tmax",
            "title": "Maximum Retry Timer",
            "description": "<p>Maximum retry timer,</p><p>specified in seconds (s), default 3600s/60min/1h.</p>",
