            "specification": {
                "type": "int",
                "definition": {
                    "min": null,
                    "max": null,
                    "unit": "s",
                    "accuracy": null
                }
            },
            "validation": {
                "default": "21600",
                "method": "area",
                "check": "{\"min\":30,\"max\":1814400}",
                "category detail": "{\"name\": \"ac_power_mode_settings\", \"title\": \"AC Power Mode\"}"
            }
        },
        {
            "identifier": "gnss_timer",
            "title": "Positioning Interval",
            "description": "<p>Positioning interval for GNSS,</p><p>specified in seconds (s), default 120s/2min.</p><p>Its limit is: [ 120 seconds, 300 seconds ].</p>",
            "category": "config",
            "specification": {
                "type": "int",
                "definition": {
                    "min": null,
                    "max": null,
                    "unit": "s",
                    "accuracy": null
                }
            },
            "validation": {
                "default": "120",
                "method": "area",
                "check": "{\"min\":120,\"max\":300}",
                "category detail": "{\"name\": \"positioning_settings\", \"title\": \"Positioning\"}"
            }
        },
        {
            "identifier": "locate",
            "title": "Positioning Mode Definition",
            "description": "<p>Positioning Mode Definition, default null,</p><p>which means to switch off positioning feature.</p>",
